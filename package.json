{
  "name": "browser-use",
  "version": "0.1.0",
  "description": "Make websites accessible for AI agents (TypeScript port)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "browser",
    "automation",
    "ai",
    "agent",
    "playwright",
    "typescript"
  ],
  "author": "Browser-Use Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yonom/browser-use-ts.git"
  },
  "dependencies": {
    "playwright": "^1.49.1",
    "zod": "^3.23.8",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "uuid": "^10.0.0",
    "minimatch": "^10.0.1",
    "node-html-markdown": "^1.3.0",
    "node-html-parser": "^6.1.13",
    "openai": "^4.77.3",
    "@anthropic-ai/sdk": "^0.30.1",
    "@google-ai/generativelanguage": "^2.8.0",
    "google-auth-library": "^9.15.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/jest": "^29.5.14",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^9.18.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}